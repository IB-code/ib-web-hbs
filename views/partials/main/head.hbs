<meta name="msapplication-TileImage" content="/public/img/fav/ms-icon-144x144.png">
<meta name="theme-color" content="#ffffff"> {{!-- /Favicon --}} {{!-- Title --}}
<title>{{default head.title "Innovate Birmingham"}}</title>
<meta property="og:title" content="{{default head.title 'Innovate Birmingham'}}"> {{!-- /Title --}}

<meta name="twitter:card" content="summary_large_image">
<meta property="fb:app_id" content="327982684338553"> {{!-- Description --}}
<meta name="description" content="{{default head.meta.description 'Default description of my website/company.'}}">
<meta property="og:description" content="{{default head.meta.description 'Default description of my website/company.'}}"> {{!-- /Description --}}

<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.1.1/css/all.css" integrity="sha384-O8whS3fhG2OnA5Kas0Y9l3cfpmYjapjI0E4theH4iuMD+pLhbf6JI0jIMfYcK3yZ"
    crossorigin="anonymous">
<!-- Remove Style -->
<link rel="stylesheet" href="/public/css/base.css">
<!-- /Remove Style -->

{{!-- Organization ld+json --}}
<script type="application/ld+json" data-sd-snippet="sd">{"@context":"http://schema.org/","@type":"Organization","name":"My Company","url":"https://mysite.com/","logo":{"@type": "ImageObject","url": "{{head.meta.baseUrl}}public/img/logos/company-text-logo-sd.png","width": 326,"height": 60},"sameAs":["https://www.facebook.com/company","https://www.instagram.com/company","https://www.linkedin.com/company","https://www.twitter.com/company"]}</script> {{!-- /Organization ld+json --}} {{!-- Breadcrumb ld+json --}}
<script type="application/ld+json" data-sd-snippet="sd">{"@context": "http://schema.org","@type": "BreadcrumbList","itemListElement": [{{#each head.meta.breadcrumbs}}{"@type": "ListItem","position": {{index}},"item": {"@id": "{{url}}","name": "{{name}}"}}{{#unless @last}},{{/unless}}{{/each}}]}</script> {{!-- /Breadcrumb ld+json --}}